<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0">
    <scr:component immediate="true" name="org.onosproject.FNL.AppComponent" activate="activate" deactivate="deactivate">
        <implementation class="org.onosproject.FNL.AppComponent"/>
        <service servicefactory="false">
            <provide interface="org.onosproject.net.NetworkTroubleshoot.NetworkTS"/>
        </service>
        <property name="service.pid" value="org.onosproject.FNL.AppComponent"/>
        <reference name="deviceService" interface="org.onosproject.net.device.DeviceService" cardinality="1..1" policy="static" bind="bindDeviceService" unbind="unbindDeviceService"/>
        <reference name="hostService" interface="org.onosproject.net.host.HostService" cardinality="1..1" policy="static" bind="bindHostService" unbind="unbindHostService"/>
        <reference name="flowRuleService" interface="org.onosproject.net.flow.FlowRuleService" cardinality="1..1" policy="static" bind="bindFlowRuleService" unbind="unbindFlowRuleService"/>
    </scr:component>
</components>
